{
  "name": "components-kit-react",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/components-kit/components-kit-react.git"
  },
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/esm/index.js"
    }
  },
  "files": [
    "lib",
    "src",
    "README.md",
    "LICENSE"
  ],
  "author": "Components Kit",
  "license": "MIT",
  "description": "Release your MVP much faster.",
  "keywords": [
    "react",
    "components",
    "ui",
    "kit"
  ],
  "homepage": "https://componentskit.com",
  "bugs": {
    "url": "https://github.com/components-kit/components-kit-react/issues"
  },
  "scripts": {
    "test": "jest",
    "build": "yarn build:esm",
    "build:esm": "tsc --module esnext",
    "format": "prettier --write \"src/**/*.tsx\"",
    "lint": "eslint \"src/**\"",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "lilconfig": "^2.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "^6.0.2",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@types/react": "18.2.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "^29.5.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "coverage",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs"
    }
  }
}
